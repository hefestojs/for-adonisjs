  "description": "Artifacts for AdonisJS!",
  "type": "module",
  "files": [
    "./build",
    "./docs/openapi",
    "./README.md",
    "./scripts/postinstall.js"
  ],
  "exports": {
    "./commands": "./build/commands/main.js",
    "./provider": "./build/app/provider.js",
    "./controller": "./build/app/controller.js",
    "./exception": "./build/app/exception.js",
    "./helper": "./build/app/helper.js",
    "./model": "./build/app/model.js",
    "./swagger": "./build/app/swagger.js"
  },
  "directories": {
